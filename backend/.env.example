# Backend API environment variables example
# Rename to .env and fill with your production values

# MySQL connection
DB_HOST=localhost
DB_PORT=3306
DB_USER=seu_usuario
DB_PASSWORD=senha_forte
DB_NAME=pecaja

# JWT secret key
JWT_SECRET=chave_super_secreta

# Cloudinary (se usar upload de imagens)
CLOUDINARY_CLOUD_NAME=cloud_name
CLOUDINARY_API_KEY=api_key
CLOUDINARY_API_SECRET=api_secret

# Outros (adicionar conforme necessidade)
# NODE_ENV=production
# LOG_LEVEL=info
